{
  "type": "object",
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "example": "2023-10-01T12:00:00Z"
          },
          "id": {
            "type": "string",
            "example": "12345"
          },
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Luke Skywalker"
                },
                "uid": {
                  "type": "string",
                  "example": "1"
                },
                "url": {
                  "type": "string",
                  "format": "uri",
                  "example": "https://www.swapi.tech/api/people/1"
                },
                "weather": {
                  "type": "object",
                  "nullable": true,
                  "properties": {
                    "description": {
                      "type": "string",
                      "example": "light rain"
                    },
                    "temp": {
                      "type": "number",
                      "example": 21.07
                    }
                  },
                  "required": ["description", "temp"]
                },
                "homeworld": {
                  "type": "string",
                  "example": "Tatooine"
                }
              }
            }
          }
        }
      }
    },
    "lastKey": {
      "type": "string",
      "nullable": true
    }
  }
}
